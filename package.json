{
  "name": "bedrock-engineer",
  "version": "1.16.4",
  "description": "Autonomous software development agent apps using Amazon Bedrock, capable of customize to create/edit files, execute commands, search the web, use knowledge base, use multi-agents, generative images and more.",
  "main": "./out/main/index.js",
  "homepage": "https://github.com/daisuke-awaji/bedrock-engineer",
  "license": "MIT-0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-samples/bedrock-engineer.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "npm run lint:eslint && npm run lint:logs",
    "lint:eslint": "node scripts/run-eslint.mjs .",
    "lint:logs": "node scripts/check-static-log-messages.mjs",
    "lint:fix": "node scripts/run-eslint.mjs . --fix",
    "test": "node scripts/run-jest.mjs --config jest.config.js --detectOpenHandles",
    "test:watch": "node scripts/run-jest.mjs --watchAll --config jest.config.js",
    "test:integration": "node scripts/run-jest.mjs --config jest.integration.config.js",
    "test:integration:watch": "node scripts/run-jest.mjs --config jest.integration.config.js --watchAll",
    "typecheck:node": "node scripts/run-tsc.mjs --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "node scripts/run-tsc.mjs --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "node scripts/run-build.mjs",
    "postinstall": "node scripts/postinstall.mjs",
    "rebuild:native": "node scripts/rebuild-native-deps.mjs",
    "clean:native": "node scripts/native-deps.mjs clean",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac --universal",
    "sign": "codesign --force --deep --sign - /path/to/YourApp.dmg",
    "sign:mac": "codesign --force --deep --sign - \"/Applications/Bedrock Engineer.app\"",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.910.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.910.0",
    "@aws-sdk/client-bedrock-runtime": "^3.910.0",
    "@aws-sdk/client-s3": "^3.910.0",
    "@aws-sdk/client-sts": "^3.910.0",
    "@aws-sdk/client-translate": "^3.910.0",
    "@aws-sdk/credential-providers": "^3.910.0",
    "@codemirror/autocomplete": "6.19.0",
    "@codesandbox/sandpack-react": "2.20.0",
    "@electron-toolkit/preload": "3.0.2",
    "@electron-toolkit/utils": "4.0.0",
    "@headlessui/react": "^2.2.9",
    "@iconify-json/logos": "1.2.9",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "@monaco-editor/react": "^4.7.0",
    "@reactour/tour": "^3.8.0",
    "@tshepomgaga/aws-sfn-graph": "0.0.6",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.12.2",
    "chart.js": "^4.5.1",
    "compression": "^1.8.1",
    "cors": "2.8.5",
    "dompurify": "^3.3.0",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.7.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "fix-path": "^5.0.0",
    "flowbite-react": "0.12.9",
    "framer-motion": "12.23.24",
    "get-port": "^7.1.0",
    "helmet": "^8.1.0",
    "https-proxy-agent": "^7.0.6",
    "i18next": "25.6.0",
    "keytar": "^7.9.0",
    "lodash": "^4.17.21",
    "lottie-react": "2.4.1",
    "mammoth": "^1.11.0",
    "mermaid": "11.12.0",
    "node-cache": "5.1.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^2.3.11",
    "proxy-addr": "^2.0.7",
    "rate-limiter-flexible": "^8.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-cmdk": "1.3.9",
    "react-drawio": "^1.0.7",
    "react-hot-toast": "2.6.0",
    "react-i18next": "16.0.1",
    "react-icons": "5.5.0",
    "react-markdown": "10.1.0",
    "react-router": "7.9.4",
    "react-router-dom": "7.9.4",
    "react-syntax-highlighter": "15.6.6",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "vite-plugin-svgr": "4.5.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.12",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@electron-toolkit/tsconfig": "2.0.0",
    "@smithy/types": "^4.7.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/compression": "^1.8.1",
    "@types/express": "5.0.3",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.7.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.2.0",
    "doctrine": "^3.0.0",
    "dotenv": "^17.2.3",
    "electron": "^38.2.2",
    "electron-builder": "^26.1.0",
    "electron-vite": "^4.0.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwindcss": "4.1.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.10"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.52.4",
    "@rollup/rollup-win32-x64-msvc": "^4.52.4",
    "dmg-license": "1.0.11"
  }
}
